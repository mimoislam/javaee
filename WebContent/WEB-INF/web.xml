<?xml version="1.0" encoding="UTF-8"?>
		<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		         version="3.0">
         
         
		         <servlet>
		         <servlet-name>Home</servlet-name>
		         <servlet-class>com.octest.servlets.HomePage</servlet-class>
		         </servlet>
         		<servlet-mapping>
         		<servlet-name>Home</servlet-name>
         		<url-pattern>/home</url-pattern>
         		</servlet-mapping>
         	<servlet>
		         <servlet-name>Login</servlet-name>
		         <servlet-class>com.octest.servlets.Login</servlet-class>
		         </servlet>
         		<servlet-mapping>
         		<servlet-name>Login</servlet-name>
         		<url-pattern>/login</url-pattern>
         		</servlet-mapping>
         		
         		
         		
         		<servlet>
		         <servlet-name>Register</servlet-name>
		         <servlet-class>com.octest.servlets.Register</servlet-class>
		         </servlet>
         		<servlet-mapping>
         		<servlet-name>Register</servlet-name>
         		<url-pattern>/register</url-pattern>
         		</servlet-mapping>
         
         
         
         
         	<servlet>
		         <servlet-name>EditNote</servlet-name>
		         <servlet-class>com.octest.servlets.EditNote</servlet-class>
		         </servlet>
         		<servlet-mapping>
         		<servlet-name>EditNote</servlet-name>
         		<url-pattern>/edit</url-pattern>
         		</servlet-mapping>
         		
         		
         		
         		
         		<servlet>
		         <servlet-name>DeleteNote</servlet-name>
		         <servlet-class>com.octest.servlets.Delete</servlet-class>
		         </servlet>
         		<servlet-mapping>
         		<servlet-name>DeleteNote</servlet-name>
         		<url-pattern>/delete</url-pattern>
         		</servlet-mapping>
         </web-app>